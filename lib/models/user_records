// To parse this JSON data, do
//
//     final welcome = welcomeFromJson(jsonString);

import 'package:meta/meta.dart';
import 'dart:convert';

Welcome welcomeFromJson(String str) => Welcome.fromJson(json.decode(str));

String welcomeToJson(Welcome data) => json.encode(data.toJson());

class Welcome {
    Welcome({
        required this.uid,
        required this.savedStories,
        required this.hashtagPreferences,
    });

    String uid;
    List<String> savedStories;
    HashtagPreferences hashtagPreferences;

    factory Welcome.fromJson(Map<String, dynamic> json) => Welcome(
        uid: json["uid"],
        savedStories: List<String>.from(json["savedStories"].map((x) => x)),
        hashtagPreferences: HashtagPreferences.fromJson(json["hashtagPreferences"]),
    );

    Map<String, dynamic> toJson() => {
        "uid": uid,
        "savedStories": List<dynamic>.from(savedStories.map((x) => x)),
        "hashtagPreferences": hashtagPreferences.toJson(),
    };
}

class HashtagPreferences {
    HashtagPreferences({
        required this.fancy,
        required this.body,
    });

    int fancy;
    int body;

    factory HashtagPreferences.fromJson(Map<String, dynamic> json) => HashtagPreferences(
        fancy: json["#fancy"],
        body: json["#body"],
    );

    Map<String, dynamic> toJson() => {
        "#fancy": fancy,
        "#body": body,
    };
}